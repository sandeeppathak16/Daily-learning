      "locked": false,
      "text": "def characterReplacement(self, s: str, k: int) -> int:\n        mapp = defaultdict(int)\n        i, j = 0, 0\n        n = len(s)\n        ans = 0\n        max_count = 0\n        \n        while j < n:\n            mapp[s[j]] += 1\n            max_count = max(max_count, mapp[s[j]])\n\n            while ((j - i) + 1) - max_count > k:\n                 mapp[s[i]] -= 1\n                 i += 1\n            \n            ans = max(ans, (j - i) + 1)\n            j += 1\n            \n        return ans",
      "fontSize": 20,
      "fontFamily": 5,
      "textAlign": "left",
      "verticalAlign": "top",
      "containerId": null,
      "originalText": "def characterReplacement(self, s: str, k: int) -> int:\n        mapp = defaultdict(int)\n        i, j = 0, 0\n        n = len(s)\n        ans = 0\n        max_count = 0\n        \n        while j < n:\n            mapp[s[j]] += 1\n            max_count = max(max_count, mapp[s[j]])\n\n            while ((j - i) + 1) - max_count > k:\n                 mapp[s[i]] -= 1\n                 i += 1\n            \n            ans = max(ans, (j - i) + 1)\n            j += 1\n            \n        return ans",
      "autoResize": true,
      "lineHeight": 1.25
